{
  "name": "fitmetrics",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rm -rf node_modules packages/*/node_modules package-lock.json",
    "api": "npm run -w packages/api",
    "mobile": "npm run -w packages/mobile",
    "dev": "npm run dev:api",
    "postinstall": "npm run prisma:generate -w packages/api && patch-package",
    "dev:api": "npm run dev -w packages/api",
    "start:mobile": "npm run start -w packages/mobile",
    "android": "npm run android -w packages/mobile",
    "ios": "npm run ios -w packages/mobile",
    "fix:mobile": "cd packages/mobile && npx expo install --fix"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "cors": "^2.8.5",
    "pg": "^8.16.3",
    "expo": "~52.0.48",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native-safe-area-context": "4.12.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-community/datetimepicker": "8.2.0",
    "@shopify/react-native-skia": "^1.12.4",
    "axios": "^1.13.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.4",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.22",
    "expo-updates": "~0.26.9",
    "expo-secure-store": "~14.0.1",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.1",
    "lucide-react-native": "^0.469.0",
    "nativewind": "^2.0.11",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.69.0",
    "react-native-calendars": "^1.1307.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/pg": "^8.16.0",
    "patch-package": "^8.0.0"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.3.1"
  },
  "version": "1.0.0"
}
